---
import AppLayout from "@/layout/AppLayout.astro";
import BannerSlider from "@/vue/components/BannerSlideShow/BannerSlider.vue";
const pageTitle: string = "Home Page";
const pageMeta: {} = [
	{
		name: "description",
		content: "This is the home page of my awesome website.",
	},
	{ name: "keywords", content: "astro, tailwind, home, awesome website" },
	{ name: "author", content: "Your Name" },
];
---

<AppLayout title={pageTitle} meta={pageMeta} path="/">
	<BannerSlider client:load />
</AppLayout>
